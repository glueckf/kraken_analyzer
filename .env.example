# Kraken Analyzer Environment Configuration
# Copy this file to .env and fill in your actual values

# Server Connection
SSH_USER=my-username
SSH_HOST=my-server.example.com
REMOTE_PATH=/path/to/my/run_results.csv

# Experiment Configuration
EXPERIMENT_LABEL=kraken1.0_vs_INES
SCHEMA_VERSION=v1

# iCloud Backup Configuration
ICLOUD_BACKUP=true
ICLOUD_DEST=/path/to/my/backup/directory

# Optional: Rsync bandwidth limit (in KB/s, uncomment to use)
# RSYNC_BWLIMIT=20000